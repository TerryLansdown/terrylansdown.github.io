---
layout: null
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>Terry Lansdown</title>
	<link href="http://terrylansdown.github.io/posts/atom.xml" rel="self"/>
	<link href="http://terrylansdown.github.io"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://terrylansdown.github.io</id>
	<author>
		<name>Terry Lansdown</name>
		<email>terry@lansdown.me</email>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="http://terrylansdown.github.io{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>http://terrylansdown.github.io/posts/{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>
